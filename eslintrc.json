{
    "parser": "@typescript-eslint/parser",
    "extends": ["plugin:@typescript-eslint/recommended", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", { "endOfLine": "auto", "no-console": "off", "no-unused-vars": "off" }],
        "@typescript-eslint/no-explicit-any": "off"
    }
}
